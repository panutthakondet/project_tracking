# =========================
# Example .env (อย่าใส่ secret จริงลง Git)
# Production แนะนำให้ตั้งค่าเป็น Environment Variables ของ Server
# =========================

# MySQL
MYSQL_CONNECTION=Server=127.0.0.1;Port=3306;Database=YOUR_DB;User=YOUR_USER;Password=YOUR_PASSWORD;

# SMTP
SMTP_SERVER=smtp.example.com
SMTP_PORT=587
SMTP_USERNAME=your_smtp_user
SMTP_PASSWORD=your_smtp_password
SMTP_ENABLE_SSL=true
SMTP_SENDER_EMAIL=noreply@example.com
